# Managed by puppet
[OUTPUT]
    Name opensearch
<%- if @match -%>
    Match <%= @match %>
<% end -%>
<% if @host -%>
    Host  <%= @host %>
<% end -%>
<% if @port -%>
    Port  <%= @port %>
<% end -%>
<% if @index -%>
    Index <%= @index %>
<% end -%>
<% if @type -%>
    Type  <%= @type %>
<% end -%>
<% if @tls -%>
    tls   <%= @tls %>
<% if @tls_verify -%>
    tls.verify   <%= @tls_verify %>
<% end -%>
<% end -%>
<% if @http_user -%>
    HTTP_User <%= @http_user %>
<% end -%>
<% if @http_passwd -%>
    HTTP_Passwd <%= @http_passwd %>
<% end -%>
<% if @logstash_format -%>
    Logstash_Format <%= @logstash_format %>
    Logstash_Prefix <%= @logstash_prefix %>
<% end -%>
<% if @suppress_type_name -%>
    Suppress_Type_Name <%= @suppress_type_name %>
<% end -%>
<% if @write_operation -%>
    Write_Operation <%= @write_operation %>
<% end -%>
